<Window x:Class="DB_Class_Generator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DB_Class_Generator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label>Server URL: </Label>
                <TextBox Name ="ServerURL" Height="20" Width="120"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label>User Name: </Label>
                <TextBox Name ="UserName" Height="20" Width="120"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label>Password:</Label>
                <PasswordBox Name ="Password" Height="20" Width="120"/>
            </StackPanel>
        </StackPanel>
  <Button Name ="Test" Margin="70,200,626,210" Content="Test Button" />
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="299,10,10,32">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
